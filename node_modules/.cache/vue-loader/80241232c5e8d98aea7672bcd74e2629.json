<div>\r\n          <el-input\r\n            v-model=\"keywords\"\r\n            placeholder=\"网址\"\r\n            style=\"width: 500px\"\r\n          ></el-input>\r\n        </div>\r\n        <div>\r\n          <el-select\r\n            v-model=\"value\"\r\n            style=\"width: 120px\"\r\n            placeholder=\"每頁顯示數量\"\r\n          >\r\n            <el-option\r\n              v-for=\"item in options\"\r\n              :key=\"item.value\"\r\n              :label=\"item.label\"\r\n              :value=\"item.value\"\r\n            >\r\n            </el-option>\r\n          </el-select>\r\n        </div>\r\n        <div>\r\n          <el-select\r\n            v-model=\"sortAsc\"\r\n            style=\"width: 120px\"\r\n            placeholder=\"佔有量排序\"\r\n          >\r\n            <el-option\r\n              v-for=\"item in sortasc\"\r\n              :key=\"item.label\"\r\n              :label=\"item.value\"\r\n              :value=\"item.label\"\r\n            >\r\n            </el-option>\r\n          </el-select>\r\n        </div>\r\n        <div><el-button type=\"primary\" @click=\"search()\">查询</el-button></div>\r\n      </div>\r\n      <div class=\"table\">\r\n        <Tables\r\n          :data=\"table.data\"\r\n          :rows=\"table.rows\"\r\n          :page=\"table.page\"\r\n          :columns=\"table.columns\"\r\n          :tableOption=\"table.tableOption\"\r\n          style=\"width: 100%\"\r\n          :loading=\"table.loading\"\r\n          :total=\"table.total\"\r\n          @clickButton=\"clickButton\"\r\n          @current-change=\"pageChange\"\r\n          @copyUrl=\"copyUrl\"\r\n          @sort-change=\"sortChange\"\r\n        >\r\n        </Tables>\r\n      </div>\r\n      <div class=\"block\">\r\n        <el-pagination\r\n          layout=\"prev, pager, next\"\r\n          background\r\n          :total=\"table.total\"\r\n          :page-size=\"table.pageCount\"\r\n          @current-change=\"pageChange\"\r\n          :current-page.sync=\"currentPage\"\r\n        >\r\n        </el-pagination>\r\n      </div>\r\n      <div class=\"addKey\">\r\n        <Dailog\r\n          title=\"设置備註欄內容\"\r\n          :width=\"'32%'\"\r\n          :height=\"310\"\r\n          :show.sync=\"keyShow\"\r\n        >\r\n          <div slot=\"content\" class=\"content\">\r\n            <el-form :model=\"form\" class=\"form\" ref=\"form\">\r\n              <el-input\r\n                type=\"textarea\"\r\n                :rows=\"10\"\r\n                maxlength=\"200\"\r\n                show-word-limit\r\n                placeholder=\"请输入備註欄內容\"\r\n                v-model=\"form.remark\"\r\n              >\r\n              </el-input>\r\n            </el-form>\r\n            <p class=\"tip\">最多可輸入200字</p>\r\n            <div class=\"footer\">\r\n              <span class=\"cancel\" @click=\"cancel\">取消</span>\r\n              <span class=\"confirm\" @click=\"submit\">保存</span>\r\n            </div>\r\n          </div>\r\n        </Dailog>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Tables from \"../../components/tables/table\";\r\nimport Dailog from \"../../components/dialogs/dialog\";\r\nimport { mapState } from \"vuex\";\r\nimport { domainremark } from \"@/api\";\r\nimport { getDomainList } from \"_api/index.js\";\r\n\r\nexport default {\r\n  name: \"AdRank\",\r\n  components: {\r\n    Tables,\r\n    Dailog,\r\n  },\r\n  data() {\r\n    return {\r\n      keywords: \"\",\r\n      value: \"10\",\r\n      sortAsc: \"desc\",\r\n      textarea: \"\",\r\n      keyText: \"\",\r\n      keyShow: false,\r\n      query: {\r\n        engine: localStorage.getItem(\"engine\"),\r\n      },\r\n      form: {\r\n        id: \"\",\r\n        remark: \"\",\r\n      },\r\n      currentPage: 1,\r\n      title: \"\",\r\n      options: [\r\n        {\r\n          value: 10,\r\n          label: \"10\",\r\n        },\r\n        {\r\n          value: 20,\r\n          label: \"20\",\r\n        },\r\n        {\r\n          value: 30,\r\n          label: \"30\",\r\n        },\r\n      ],\r\n      sortasc: [\r\n        {\r\n          value: \"排序低\",\r\n          label: \"asc\",\r\n        },\r\n        {\r\n          value: \"排序高\",\r\n          label: \"desc\",\r\n        },\r\n      ],\r\n      table: {\r\n        loading: false,\r\n        page: 1,\r\n        rows: 30,\r\n        pageCount: 10,\r\n        total: 0,\r\n        columns: [\r\n          {\r\n            label: \"排名\",\r\n            key: \"id\",\r\n          },\r\n          {\r\n            label: \"域名\",\r\n            key: \"domain\",\r\n\r\n          },\r\n          {\r\n            label: \"排名佔有量\",\r\n            key: \"pm_count\",\r\n            sortable: true\r\n          },\r\n          {\r\n            label: \"PC权重\",\r\n            key: \"baidu_pc_br\",\r\n            sortable: true\r\n          },\r\n          {\r\n            label: \"PC预估流量\",\r\n            key: \"baidu_pc_uvcount\",\r\n            sortable: true\r\n          },\r\n          {\r\n            label: \"移动权重\",\r\n            key: \"baidu_m_br\",\r\n            sortable: true\r\n          },\r\n          {\r\n            label: \"移动预估流量\",\r\n            key: \"baidu_m_uvcount\",\r\n            sortable: true\r\n          },\r\n          {\r\n            label: \"建站时间\",\r\n            key: \"created_at\",\r\n            sortable: true\r\n          },\r\n          {\r\n            label: \"備註\",\r\n            key: \"remark\",\r\n            \r\n          },\r\n        ],\r\n        data: [],\r\n        // 表格操作\r\n        tableOption: {\r\n          label: \"操作\",\r\n          options: [\r\n            {\r\n              label: \"编辑\",\r\n              bgColor: \"#5bbfdf\",\r\n              methods: \"edit\",\r\n            },\r\n          ],\r\n        },\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    moment: function () {\r\n      return moment();\r\n    },\r\n    getData(params) {\r\n      this.table.loading = true;\r\n\r\n      getDomainList(params)\r\n        .then((res) => {\r\n          if (res != undefined && res.code == 200) {\r\n            this.table.data = res.data.data;\r\n            this.table.total = res.data.total;\r\n\r\n\r\n            this.table.data.forEach((item) => {\r\n              item.created_at = this.$moment(item.created_at * 1000).format(\r\n                \"YYYY-MM-DD\"\r\n              );\r\n            });\r\n            this.table.loading = false;\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          this.table.data = [];\r\n          this.table.total = 0;\r\n          this.table.loading = false;\r\n        });\r\n    },\r\n    // 翻页\r\n    pageChange(val) {\r\n      this.table.page = val;\r\n      this.params.page = val;\r\n      this.getData(this.params);\r\n    },\r\n    // 点击排序\r\n    sortChange(val) {\r\n      this.\r\n      console.log(val)\r\n    },\r\n    search() {\r\n      this.currentPage = 1;\r\n      this.params.page = 1;\r\n      this.table.sortAsc = this.sortAsc;\r\n      this.params.sortAsc = this.sortAsc;\r\n      this.table.domain = this.keywords;\r\n      this.params.domain = this.keywords;\r\n      this.table.pageCount = this.value;\r\n      this.params.pageCount = this.value;\r\n      this.getData(this.params);\r\n      console.log()\r\n    },\r\n    clickButton(val) {\r\n      // console.log(val)\r\n      this.title = \"编辑\";\r\n      this.keyShow = true;\r\n      this.form.id = val.row.id;\r\n      this.form.remark = val.row.remark;\r\n    },\r\n    openKey() {\r\n      this.keyShow = true;\r\n    },\r\n    submit() {\r\n      var bodyFormData = new FormData();\r\n      bodyFormData.append(\"id\", this.form.id);\r\n      bodyFormData.append(\"remark\", this.form.remark);\r\n      domainremark(bodyFormData).then((res) => {\r\n        if (res.code === 200 && res.status === \"success\") {\r\n          this.addKey = false;\r\n          this.$message({\r\n            message: \"编辑关键字成功！\",\r\n            type: \"success\",\r\n          });\r\n          this.params.page = 1;\r\n          this.getData(this.params);\r\n          this.keyShow = false;\r\n        } else if (res.code === 400 && res.status === \"error\") {\r\n          this.addKey = false;\r\n          this.$message({\r\n            message: \"備註欄位不可為空白！\",\r\n            type: \"success\",\r\n          });\r\n        }\r\n      });\r\n    },\r\n    cancel() {\r\n      this.keyText = \"\";\r\n      this.keyShow = false;\r\n    },\r\n    copyUrl({raw,column}){\r\n      let copyUrl = raw.domain;\r\n       if(column.label == \"域名\") {\r\n        if(copyUrl == \"\") {\r\n          retutn;\r\n        }else {\r\n          let url = document.createElement(\"input\");\r\n          document.body.appendChild(url);\r\n          url.value = copyUrl;\r\n          url.select();\r\n          document.execCommand(\"copy\");\r\n          document.body.removeChild(url);\r\n          this.$message({\r\n            message:'网址复制成功',\r\n            type:'success',\r\n            duration:1000\r\n          })\r\n        }\r\n      }\r\n\r\n    },\r\n  },\r\n  created() {\r\n    this.getData(this.params);\r\n  },\r\n  mounted() {\r\n    this.params = {\r\n      sortBy: \"pm_count\",\r\n      sortAsc: \"desc\",\r\n      domain: \"\",\r\n      page: 1,\r\n      pageCount: 10,\r\n      engine: this.query.engine,\r\n    };\r\n    this.getData(this.params);\r\n  },\r\n  computed: {\r\n    ...mapState([\"token\", \"engine\"]),\r\n  },\r\n  watch: {\r\n    engine(n, o) {\r\n      // console.log(`new:${n}`);\r\n      // console.log(`new:${o}`);\r\n      this.params.engine = n;\r\n      // console.log(this.query.engine)\r\n      this.getData(this.params);\r\n\r\n      this.$forceUpdate();\r\n    },\r\n  },\r\n};\r\n</script>\r\n   \r\n<style lang=\"scss\" scoped>\r\n.tableHead {\r\n  display: flex;\r\n  width: 65vw;\r\n  margin: 0 auto;\r\n  padding: 41px 0 !important;\r\n  border-bottom: 1px solid #eaeaea;\r\n  div {\r\n    margin: 0 8px;\r\n    &:first-child {\r\n      margin-left: 0px;\r\n    }\r\n    &:last-child {\r\n      margin-right: 0px;\r\n    }\r\n    /deep/.el-pagination {\r\n      display: none;\r\n    }\r\n  }\r\n}\r\n.content {\r\n  padding: 0 26px 1vh 26px;\r\n  .table {\r\n    width: 66vw;\r\n    margin: 0 auto;\r\n    padding: 10px 0 21px 0;\r\n    /deep/.el-button--mini {\r\n      background: #5bc0de;\r\n      color: #fff;\r\n    }\r\n    /deep/.el-pagination {\r\n      display: none;\r\n    }\r\n    /deep/.btn {\r\n      margin-right: 0;\r\n    }\r\n    /deep/thead{\r\n      font-size: 11px;\r\n      .caret-wrapper{\r\n        font-size:11px ;\r\n      }\r\n    }\r\n    /deep/tbody{\r\n      font-size: 11px;\r\n    }\r\n  }\r\n}\r\n.content {\r\n  .addKey {\r\n    /deep/.el-dialog__wrapper {\r\n      top: 8%;\r\n    }\r\n    .tip {\r\n      margin: 15px 0px 0px 16px;\r\n      font-size: 14px;\r\n      font-weight: 300;\r\n      line-height: 0.86;\r\n      color: #868e96;\r\n    }\r\n    .footer {\r\n      display: flex;\r\n      justify-content: flex-end;\r\n      span {\r\n        width: 65px;\r\n        height: 38px;\r\n        line-height: 38px;\r\n        opacity: 0.76;\r\n        border-radius: 4px;\r\n        text-align: center;\r\n        font-size: 16px;\r\n        cursor: pointer;\r\n      }\r\n      .cancel {\r\n        border: solid 1px #868e96;\r\n        background-color: #ffffff;\r\n        margin-right: 20px;\r\n      }\r\n      .confirm {\r\n        border: solid 1px #5595c8;\r\n        background-color: #66afe9;\r\n        color: #fff;\r\n      }\r\n    }\r\n  }\r\n}\r\n/deep/.gutter {\r\n  display: initial !important;\r\n}\r\n/deep/.block {\r\n  margin-top: 3%;\r\n}\r\n.heightcontent {\r\n  width: 69vw;\r\n  background: #fff;\r\n  border-radius: 5px;\r\n}\r\n/deep/.el-table .caret-wrapper{\r\n  width: 8px;\r\n}\r\n</style>"]}]}